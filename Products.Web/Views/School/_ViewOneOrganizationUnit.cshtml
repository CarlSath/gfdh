@model Products.Web.Models.SchoolModel

@Html.Hidden("EditOneOrganizationUnit.Id")
@Html.Hidden("EditOneOrganizationUnit.OwnerId")
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label for="Name" class="col-md-4 from-title">Name</label>
            <div class="col-md-8 from-cont">
                @Html.TextBox("EditOneOrganizationUnit.Name", null, new {@readonly = "readonly", @class = "readonly"})
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label for="Description" class="col-md-4 from-title">Description</label>
            <div class="col-md-8 from-cont">
                @Html.TextBox("EditOneOrganizationUnit.Description", null, new {@readonly = "readonly", @class = "readonly"})
            </div>
        </div>
    </div>
</div>

<div class="submit">
    <input type="button" value="Cancel" class="cancel rightAlign" onclick="MetaShare.Modal.prototype.closeModal('editOrganizationUnit')"/>
</div>